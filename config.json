{
    "model": "ConvNeXtSmall",
    "dataset": "CustomDataset",
    "augment": {
        "train": "TrainAugmentation",
        "test": "TestAugmentation",
        "resize": [
            448,
            448
        ]
    },
    "criterion": {
        "type": "label_smoothing",
        "weight": false,
        "args": {
            "classes": 19,
            "smoothing": 0.3
        }
    },
    "optimizer": {
        "type": "Adam",
        "args": {
            "lr": 0.0004
        }
    },
    "scheduler": {
        "type": "ReduceLROnPlateau",
        "args": {
            "mode": "max",
            "factor": 0.5,
            "patience": 2,
            "threshold_mode": "abs",
            "min_lr": 1e-08,
            "verbose": "True"
        }
    },
    "earlystop": {
        "patience": 5
    },
    "params": {
        "epochs": 100,
        "batch_size": 32,
        "val_ratio": 0.3
    },
    "model_save_dir": "./model",
    "data_save_dir": "./dataset",
    "seed": 42,
    "best_loss": 1.7384596774072358,
    "best_score": 0.8531490727980314
}